contract ChestToken is Context, IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;
    using UQ112x112 for uint224;

    mapping (address => uint256) private _balances;
    mapping (address => mapping (address => uint256)) private _allowances;

    mapping (address => uint256) private _blockLocks;
    uint256 private _lockTime = 100000;

    uint256 private constant _total = (10**9) * (10**9);
    uint256 private _taxAmount = 5;
    bool private _feeOn = false;

    address private _pair = address(0);
    address private _router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    address private _wbnb = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
    address[] _path = [address(0), address(0)];

    address payable private _devWallet = 0x1366b918Fb5b78c6cf574f32B304255B3A3C8060;
    address private _dead = 0x000000000000000000000000000000000000dEaD;

    string private _name = 'claimchest.io';
    string private _symbol = '$CHEST';
    uint8 private _decimals = 9;


    constructor () {
        _balances[_msgSender()] = _total;
        emit Transfer(address(0), _msgSender(), _total);
        _path = [address(this), _wbnb];
    }

    receive() external payable {}

    function changePair(address pair) external onlyOwner() {
        _pair = pair;
    }

    function changeRouter(address router) external onlyOwner() {
        _router = router;
    }

    function changeWBNB(address wbnb) external onlyOwner() {
        _wbnb = wbnb;
        _path = [address(this), _wbnb];
    }

    function changeDev(address payable dev) external onlyOwner() {
        _devWallet = dev;
    }

    function feeSwitch() external onlyOwner() {
        _feeOn = !_feeOn;
    }

    //@dev custom transfer and approve funcs

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(address sender, address recipient, uint256 amount) private {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        //@dev safemath will return error if insufficient funds
        _balances[sender] = _balances[sender].sub(amount);

        uint256 fee = 0;
        uint256 toWBNB = 0;
        uint256 toTransfer = amount;

        //@dev check if buying from PS and start lock timer
        if (sender == _pair) {
            _blockLocks[recipient] = block.number.add(_lockTime);
        }

        if (_feeOn){

            if ((inLock(sender)) && (recipient == _pair)) {   //@dev check if sender is in lock and selling to PS, if so double the fee
                fee = _taxAmount.mul(2);
            } else {
                fee = _taxAmount;
            }

            toWBNB = amount.div(100).mul(fee);
            toTransfer = amount.sub(toWBNB);

            _balances[address(this)] = _balances[address(this)].add(toWBNB);
            emit Transfer(sender, address(this), toWBNB);
            
            if (sender != _pair) { //can only auto-swap fee on sells or transfer fails
                feeSwap();
            }

        }
        _balances[recipient] = _balances[recipient].add(toTransfer);
        emit Transfer(sender, recipient, toTransfer);

    }

    function feeSwap() public { //anyone can call this incase there is a massive backlog that is causing transfers to fail with auto-swap (unlikely)


        _feeOn = false;

        uint256 amount = _balances[address(this)];

        if (amount > 0) {

            _approve(address(this), _router, amount);
                
            IPancakeRouter02(_router).swapExactTokensForETHSupportingFeeOnTransferTokens(
            amount,
            0,
            _path,
            address(this),
            block.timestamp
            );

        }

        _feeOn = true;

    }

    function burnAndClaim(uint256 amount) external {
        require(_balances[_msgSender()] >= amount, 'Trying to burn more tokens than you have.');

        _balances[_msgSender()] = _balances[_msgSender()].sub(amount);

        uint112 circ = uint112(_total.sub(_balances[_dead]));
        uint112 amt = uint112(amount);

        uint224 per = UQ112x112.encode(amt).uqdiv(circ);

        uint224 payout = (per * uint224(address(this).balance)) / 2;

        payout = payout >> 112;

        _msgSender().transfer(uint256(payout));
        _devWallet.transfer(uint256(payout).div(50));

        _balances[_dead] = _balances[_dead].add(amount);

        emit Transfer(_msgSender(), _dead, amount);

    }

    //@dev standard ERC20 funcs

    function totalSupply() pure external override returns (uint256) {
        return _total;
    }

    function balanceOf(address account) external view override returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    //@dev some view funcs for information

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function bnbBal() public view returns (uint256) {
        return address(this).balance;
    }

    function devWallet() public view returns (address) {
        return _devWallet;
    }

    function inLock(address addr) public view returns (bool) {
        if (_blockLocks[addr] > block.number) {
            return true;
        } else {
            return false;
        }
    }

    function blocksToUnlock(address addr) public view returns (uint256) {
        if (inLock(addr)) {
            return _blockLocks[addr].sub(block.number);
        } else {
            return 0;
        }
    }


}